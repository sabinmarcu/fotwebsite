#Posts > div > material-content.loader
    left (asidewidth / 3 * 2)
article.post
    width calc(50% - 14.5px) !important
    max-width calc(50% - 14.5px) !important
    margin 0 !important
    margin-bottom 25px !important
    &:nth-child(3n + 1)
        margin-left auto
    &:nth-child(3n)
        margin-right auto
    &:nth-child(9n - 6),&:nth-child(9n - 4), &:nth-child(9n - 2)
        width auto
        max-width auto
    &:nth-child(2n + 1)
        margin-left 0 !important
        margin-right 12.5px !important
    &:nth-child(2n)
        margin-right 0 !important
        margin-left 12.5px !important